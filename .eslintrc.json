{
   "env": {
      "browser": true,
      "jquery": true
   },
   "globals": {},
   "parser": "babel-eslint",
   "rules": {
      "indent": ["error", 3, { "SwitchCase": 1 }],
      "brace-style": ["error", "stroustrup"],
      "implicit-arrow-linebreak": ["off"],
      "no-underscore-dangle": "off",
      "no-plusplus": "off"
   },
   "extends": ["airbnb-base"]
}
